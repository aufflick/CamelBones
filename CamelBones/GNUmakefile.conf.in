# -*- Autoconf -*-

# Package information
PACKAGE_NAME = @PACKAGE_NAME@
VERSION = @PACKAGE_VERSION@

# The full path to perl
PERL = @PERL@

# The full path to libperl (usually optional)
LIBPERL = @LIBPERL@

# Compiler and friends
CC = @CC@
OBJC = @CC@
LIBTOOL = @LIBTOOL@

# Use local ffcall, or build one
HAVE_LIBAVCALL = @HAVE_LIBAVCALL@

# Allow debugging
DEBUG = @DEBUG@

# Build an embedded framework
EMBEDDED = @EMBEDDED@

# Build a universal framework
UNIVERSAL = @UNIVERSAL@

# Delete existing framework when installing
CLEAN_INSTALL = @CLEAN_INSTALL@

# Overwrite stub framework when installing
OVERWRITE_STUB = @OVERWRITE_STUB@

# Install volume
INSTALL_VOLUME = @INSTALL_VOLUME@

# SDK to build against (Mac OS X only)
SDK = @SDK@

# Location in which to install prebuilt module "kits"
KITS_PATH = @KITS_PATH@

# Whether to build PAR support
ENABLE_PAR = @ENABLE_PAR@

# Whether to build kits
ENABLE_KITS = @ENABLE_KITS@
